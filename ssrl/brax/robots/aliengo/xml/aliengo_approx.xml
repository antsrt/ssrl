        <numeric data="0.0 0.0 0.34 1.0 0.0 0.0 0.0 -0.01014303 0.7180088 -1.4360176 0.01014303 0.7180088 -1.4360176 -0.01014303 0.7180088 -1.4360176 0.01014303 0.7180088 -1.4360176" name="init_qpos"/>
